<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/window_background_color_primary"
    tools:context=".dashboard.fragments.HomeFragment">

    <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
        android:id="@+id/contacts"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="25dp"
        android:tooltipText="Add contact"
        app:icon="@drawable/ic_add_contact"
        app:iconTint="@color/white"
        app:backgroundTint="@color/tool_bar_color"
        app:elevation="25dp"
        app:extendStrategy="wrap_content"
        tools:targetApi="o" />

</RelativeLayout>